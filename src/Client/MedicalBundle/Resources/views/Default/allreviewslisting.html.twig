<script type="text/javascript">$(document).ready(function () {	var cboxOptions = {  				width: '95%',				height: '95%',				maxWidth: '960px',				maxHeight: '960px',			}	$('.ajax').colorbox(cboxOptions);	$(window).resize(function(){	  $.colorbox.resize({		width: window.innerWidth > parseInt(cboxOptions.maxWidth) ? cboxOptions.maxWidth : cboxOptions.width,		height: window.innerHeight > parseInt(cboxOptions.maxHeight) ? cboxOptions.maxHeight : cboxOptions.height	  });	});});</script><div class="col-md-8 col-xs-8 companylist_container bannerheight"><h4 class="fourth_heading">{{'TESTIMONIALS'|trans}}</h4><div id="loader" style="display:none"></div><div id="news_div">	  {% if pagination.getTotalItemCount > 0%}	    {% for asDatas in pagination %}    <div class="news_row row">	<div class="col-md-2 col-xs-2 offer_img">		{% if asset_exists('uploads/company/'~asDatas.company_id~'/'~asDatas.photo) %}							<a  class="ajax" href="{{ path('client_medical_rating', {'id': asDatas.company_id} )}}"><img src="{{ asset('uploads/company/'~asDatas.company_id~'/'~asDatas.photo) }}" /></a>		{% else %}			<a  class="ajax" href="{{ path('client_medical_rating', {'id': asDatas.company_id} )}}"><img src="{{ asset('client/images/no_image.png') }}" alt="No Image"/></a>		{% endif %}	</div>	<div class="col-md-10 col-xs-10">	           	  <div class="row offer_detils">		<h3><a class="ajax" href="{{ path('client_medical_rating', {'id': asDatas.company_id} )}}"><em>{{ asDatas.name~" "~asDatas.lastname }}</em></a></h3>	    <div class="col-xs-6 col-lg-4 ratting_stars">            			{% set data = '%.2f' | format(asDatas.total_avg) %}			{% include 'ClientMedicalBundle:Default:ratingstar.html.twig' %}		</div>		<span class="offer_date">{{ asDatas.rating_date|date('d m Y') }}</span>							{% if asDatas.comments|length > 115 %}			{% set data = asDatas.comments|striptags %}			<p>{{ data|slice(1,115)}}			<a class="ajax" href="{{ path('client_medical_rating', {'id': asDatas.company_id} )}}">{{'Read more...'|trans}}</a>					{% else %}			<p>{{ asDatas.comments|striptags }}</p>		{% endif %}	  </div>	 	</div>	  	</div>  {% endfor %}  {{ knp_pagination_render(pagination) }}   </div>  {% endif %} </div>	{% set route = path(ssRoute) %}<div class="col-md-3 right_part">	<div class="news_banner_wrapper">		{{ render(controller('ClientMedicalBundle:News:banners',{'ssPage':'news'})) }}	</div>		{{ render(controller('ClientMedicalBundle:Company:specialoffers',{'ssRoute':route})) }}</div>