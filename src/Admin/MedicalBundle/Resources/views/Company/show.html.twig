{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Company</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Company_code</th>
                <td>{{ entity.companycode }}</td>
            </tr>
            <tr>
                <th>Vat_code</th>
                <td>{{ entity.vatcode }}</td>
            </tr>
            <tr>
                <th>Licence_number</th>
                <td>{{ entity.licencenumber }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ entity.address }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ entity.city }}</td>
            </tr>
            <tr>
                <th>Zip</th>
                <td>{{ entity.zip }}</td>
            </tr>
            <tr>
                <th>Disable_quotes</th>
                <td>{{ entity.disablequotes }}</td>
            </tr>
            <tr>
                <th>Primary_phone</th>
                <td>{{ entity.primaryphone }}</td>
            </tr>
            <tr>
                <th>Cell_phone</th>
                <td>{{ entity.cellphone }}</td>
            </tr>
            <tr>
                <th>Fax</th>
                <td>{{ entity.fax }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Company_note</th>
                <td>{{ entity.companynote }}</td>
            </tr>
            <tr>
                <th>Language</th>
                <td>{{ entity.language }}</td>
            </tr>
            <tr>
                <th>Badges</th>
                <td>{{ entity.badges }}</td>
            </tr>
            <tr>
                <th>Payment_option</th>
                <td>{{ entity.paymentoption }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ entity.createdat|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Updated_at</th>
                <td>{{ entity.updatedat|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('medical_company') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('medical_company_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
